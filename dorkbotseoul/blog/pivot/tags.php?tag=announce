				$("#tgrrsslist").html('<img src="/dorkbotseoul/blog/pivot/pics/taggerati/loading_' + type + '.gif" alt=""/>');
				var url = "/dorkbotseoul/blog/pivot/includes/ajax_rsslist.php?w=my_weblog";
				$.get( url, { type: type, tag: tag }, function(output) { $("#tgrrsslist").html(output); } );
